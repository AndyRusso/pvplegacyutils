{
  "schemaVersion": 1,
  "id": "pvplegacyutils-1-19",
  "version": "${version}",

  "name": "PvP Legacy Utils for 1.19 to 1.19.2",
  "description": "This mod is used to make PvP Legacy Utils work for 1.19 & 1.19.1 & 1.19.2",
  "authors": [
    "Andy Russo"
  ],

  "license": "GPL-3.0-or-later",
  "icon": "assets/pvplegacyutils-1-19/icon.png",

  "environment": "client",
  "entrypoints": {
    "pvplegacyutils-versioned": [
      "io.github.andyrusso.pvplegacyutils.versions.mc1_19.PvPLegacyUtilsVersioned"
    ]
  },

  "mixins": [
    "pvplegacyutils-1-19.mixins.json"
  ],

  "accessWidener" : "pvplegacyutils-1-19.accesswidener",

  "depends": {
    "fabricloader": ">=0.14.0",
    "fabric": ">=0.55.1",
    "minecraft": ">=1.19 <1.19.3-",
    "java": ">=17"
  },

  "custom": {
    "modmenu": {
      "parent": "pvplegacyutils"
    }
  }
}
